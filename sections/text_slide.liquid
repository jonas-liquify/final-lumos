<div class="u-embed-js w-embed w-script">
          <script>
document.addEventListener('DOMContentLoaded', function() {
  const rotationWrap = document.querySelector('.text-slide_rotation_txt_wrap');
  if (!rotationWrap) return;
  const items = Array.from(rotationWrap.querySelectorAll('.text-slide_rotation_txt'));
  if (items.length === 0) return;
  items.forEach(item => {
    item.style.paddingBottom = 'var(--_spacing---space--7)';
  });
  const track = document.createElement('div');
  track.className = 'text-slide_rotation_track';
  track.style.willChange = 'transform';
  items.forEach(item => {
    track.appendChild(item);
  });
  rotationWrap.innerHTML = '';
  rotationWrap.appendChild(track);
  rotationWrap.style.overflow = 'hidden';
  rotationWrap.style.position = 'relative';
  items.forEach(item => {
    const clone = item.cloneNode(true);
    clone.style.paddingBottom = 'var(--_spacing---space--7)';
    track.appendChild(clone);
  });
  const totalItems = items.length;
  let tl;
  function initAnimation() {
    if (tl) {
      tl.kill();
    }
    gsap.set(track, { y: 0, clearProps: 'all' });
    track.style.willChange = 'transform';
    const itemHeight = items[0].offsetHeight;
    rotationWrap.style.height = (itemHeight * 3) + 'px';
    tl = gsap.timeline({ repeat: -1 });
    for (let i = 0; i < totalItems; i++) {
      tl.to(track, {
        y: -(itemHeight * i),
        duration: 0.6,
        ease: "power2.inOut",
        force3D: true
      })
      .to({}, { duration: 2 });
    }
    tl.to(track, {
      y: -(itemHeight * totalItems),
      duration: 0.6,
      ease: "power2.inOut",
      force3D: true
    })
    .call(() => {
      gsap.set(track, { y: 0 });
    });
  }
  initAnimation();
  let resizeTimer;
  let resizeRAF;
  let lastWidth = window.innerWidth;
  window.addEventListener('resize', function() {
    const currentWidth = window.innerWidth;
    if (currentWidth !== lastWidth) {
      cancelAnimationFrame(resizeRAF);
      clearTimeout(resizeTimer);
      resizeRAF = requestAnimationFrame(function() {
        resizeTimer = setTimeout(function() {
          initAnimation();
          lastWidth = currentWidth;
        }, 250);
      });
    }
  }, { passive: true });
});
</script>
