{% if predictive_search.performed %}    <div li-if="predictive_search.performed" class="predictive-search_conditional">
    
                                  {% assign first_column_results_size = predictive_search.resources.queries.size | plus: predictive_search.resources.collections.size | plus: predictive_search.resources.pages.size | plus: predictive_search.resources.articles.size | plus: predictive_search.resources.products.size %}
                                  {% if first_column_results_size > 0 %}    <div li-if="first_column_results_size > 0" class="predictive-search_conditional">
    
                                    {% if predictive_search.resources.queries.size > 0 or predictive_search.resources.collections.size > 0 %}    <div li-if="predictive_search.resources.queries.size > 0 or predictive_search.resources.collections.size > 0" class="predictive-search_topic">
    
                                      <div class="predictive-search_headline">
                                        <div class="predictive-search_headline_txt u-text-style-h6 u-color-faded">Suggestions</div>
                                      </div>
                                      <ul role="list" class="predictive-search_list w-list-unstyled">
                                        {% for query in predictive_search.resources.queries %}    <li li-for="query in predictive_search.resources.queries" class="predictive-search_item">
                                    
                                          
                            <a li-object:href="query.url" href="{{ query.url }}" class="predictive-search_link w-inline-block">
                                            <div li-object="query.styled_text" class="predictive-search_tag u-text-style-h6">{{ query.styled_text }}</div>
                                          </a>
                            
                                        
                        </li>
{% endfor %}
                                        {% for collection in predictive_search.resources.collections %}    <li li-for="collection in predictive_search.resources.collections" class="predictive-search_item">
                                    
                                          
                            <a li-object:href="collection.url" href="{{ collection.url }}" class="predictive-search_link w-inline-block">
                                            <div li-object="collection.title" class="predictive-search_tag u-text-style-h6">{{ collection.title }}</div>
                                          </a>
                            
                                        
                        </li>
{% endfor %}
                                      </ul>
                                        </div>
{% endif %}
                                    {% if predictive_search.resources.products.size > 0 or predictive_search.resources.articles.size > 0 or predictive_search.resources.pages.size > 0 %}    <div li-if="predictive_search.resources.products.size > 0 or predictive_search.resources.articles.size > 0 or predictive_search.resources.pages.size > 0" class="predictive-search_topic">
    
                                      <div class="predictive-search_headline">
                                        <div class="predictive-search_headline_txt u-text-style-h6 u-color-faded">Products</div>
                                      </div>
                                      <div role="list" class="predictive-search_product-list">
                                        {% for product in predictive_search.resources.products %}    <div role="listitem" li-for="product in predictive_search.resources.products" class="predictive-search_product-item">
                                    
                                          
                            <script src="{{ 'li_custom.js' | asset_url }}" type="text/javascript"></script>
<div li-element="product-variant-container" class="predictive-search_container" x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})" x-init="$nextTick(() => { activateVariants() })" @setbundleitem.window="updateBundleItem($event.detail.product, $event.detail.quantity)">
                                            <div class="predictive-search_product-item_styles" style="--flavor-bg: {{ product.metafields.custom.flavor_metaobject.value.background_color }};">
                                              <div class="predictive-search_line-item">
                                                <div class="predictive-search_image-wrapper">
                                                  <div class="product-item_radial is-mini"></div><img li-object:src="product.featured_media | image_url: width: 150" li-object:alt="product.featured_image.alt" alt="{{ product.featured_image.alt }}" src="{{ product.featured_media | image_url: width: 150 }}" loading="lazy" class="predictive-search_image">
                                                </div>
                                                <div class="product-item_bg is-mini"></div>
                                                <div class="predictive-search_product-info">
                                                  <div class="predictive-search_product_top">
                                                    <p data-wf--eyebrow--variant="small" li-object="product.metafields.custom.flavor_metaobject.value.flavor" class="g_eyebrow_txt u-text-style-h6 w-variant-2ea84ea1-fdee-b528-5a8e-fd7b483b99f5" style="background-color: {{ product.metafields.custom.flavor_metaobject.value.background_color }}; color: {{ product.metafields.custom.flavor_metaobject.value.text_color }};">{{ product.metafields.custom.flavor_metaobject.value.flavor }}</p>
                                                    <p data-wf--eyebrow--variant="small" class="g_eyebrow_txt u-text-style-h6 w-variant-2ea84ea1-fdee-b528-5a8e-fd7b483b99f5">24 Packs</p>
                                                  </div>
                                                  <div class="predictive-search_product_center">
                                                    <div li-object="product.title" class="predictive-search_product_title u-text-style-h6">{{ product.title }}</div>
                                                    <div li-object="product.metafields.custom.tagline" class="predictive-search_product_byline u-text-style-h6 u-weight-medium u-color-faded">{{ product.metafields.custom.tagline }}</div>
                                                  </div>
                                                  <div class="clickable_wrap u-cover-absolute">
                                                    <a li-object:href="product.url" class="clickable_link" href="{{ product.url }}"><span li-object="product.title" class="clickable_text u-sr-only">{{ product.title }}</span></a><button type="button" class="clickable_btn"><span li-object="product.title" class="clickable_text u-sr-only">{{ product.title }}</span></button>
                                                  </div>
                                                  <div class="predictive-search_product_bottom">
                                                    <div data-wf--button-main--variant="primary" class="button_main_wrap" data-button=" " data-trigger="hover focus">
                                                      <div class="clickable_wrap u-cover-absolute">
                                                        <a li-element="add-to-cart" class="clickable_link" @click="addToCart($event, true)" :disabled="!addToCartButton"><span class="clickable_text u-sr-only">Add to Cart</span></a><button type="button" class="clickable_btn"><span class="clickable_text u-sr-only">Add to Cart</span></button>
                                                      </div>
                                                      <div class="button_main_element">
                                                        <p aria-hidden="true" class="button_main_text u-text-style-h6">Add to Cart</p>
                                                        <div class="button_main_icon u-hide-if-empty"></div>
                                                        <div class="button_main_line"></div>
                                                      </div>
                                                      <div class="u-display-contents"></div>
                                                    </div>
                                                    <div class="predictive-search_price_wrap">
                                                      <div li-object="product.price | money" class="predictive-search_price u-text-style-small">{{ product.price | money }}</div>
                                                      {% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price">
    
                                                        <div li-object="product.compare_at_price | money" class="predictive-search_compare-price u-text-style-small u-color-faded">{{ product.compare_at_price | money }}</div>
                                                          </div>
{% endif %}
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                            </div>
                                          </div>
                            
                                        
                        </div>
{% endfor %}
                                      </div>
                                        </div>
{% endif %}
                                    {% if predictive_search.resources.articles.size > 0 or predictive_search.resources.pages.size > 0 %}    <div li-if="predictive_search.resources.articles.size > 0 or predictive_search.resources.pages.size > 0" class="predictive-search_topic">
    
                                      <div class="predictive-search_headline">
                                        <div class="predictive-search_headline_txt u-text-style-h6 u-color-faded">Pages</div>
                                      </div>
                                      <ul role="list" class="predictive-search_list w-list-unstyled">
                                        {% for page in predictive_search.resources.pages %}    <li li-for="page in predictive_search.resources.pages" class="predictive-search_item">
                                    
                                          
                            <a li-object:href="page.url" href="{{ page.url }}" class="predictive-search_link w-inline-block">
                                            <div li-object="page.title" class="predictive-search_tag u-text-style-h6">{{ page.title }}</div>
                                          </a>
                            
                                        
                        </li>
{% endfor %}
                                      </ul>
                                        </div>
{% endif %}
                                      </div>
{% endif %}
                                  {% if first_column_results_size == 0 %}    <div li-if="first_column_results_size == 0" class="predictive-search_conditional">
    
                                    <div class="predictive-search_topic">
                                      <div class="predictive-search_topic_txt u-text-style-main">Nothing found in "<span li-object="predictive_search.terms">{{ predictive_search.terms }}</span>"</div>
                                    </div>
                                      </div>
{% endif %}
                                    </div>
{% endif %}
                                {% unless predictive_search.performed %}    <div li-unless="predictive_search.performed" class="predictive-search_conditional">
    
                                  <div class="predictive-search_overflow">
                                    <div class="predictive-search_topic">
                                      <div class="predictive-search_headline">
                                        <div class="predictive-search_headline_txt u-text-style-h6 u-color-faded">Suggestions</div>
                                      </div>
                                      <ul role="list" class="predictive-search_list w-list-unstyled">
                                        {% for block in section.blocks %}
    {% if block.type == "search-suggestion" %}
                    <li li-block="Search - Suggestion" class="predictive-search_item">
                                    
                                          
                                    <a href="{{ block.settings.url_link }}" class="predictive-search_link w-inline-block"> <div class="predictive-search_tag u-text-style-h6">{{ block.settings.text_text }}</div> </a>
                                    
                                        
                            </li>
            {% endif %}
{% endfor %}
                                      </ul>
                                    </div>
                                    <div class="predictive-search_topic">
                                      <div class="predictive-search_headline">
                                        <div class="predictive-search_headline_txt u-text-style-h6 u-color-faded">Bestseller</div>
                                      </div>
                                      <div role="list" class="predictive-search_product-list">
                                        {% for product in section.settings.collection_search_bestseller.products limit: section.settings.collection_search_bestseller_limit %}
<div role="listitem" class="predictive-search_product-item">
    
                                          
    <script src="{{ 'li_custom.js' | asset_url }}" type="text/javascript"></script>
<div li-element="product-variant-container" class="predictive-search_container" x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})" x-init="$nextTick(() => { activateVariants() })" @setbundleitem.window="updateBundleItem($event.detail.product, $event.detail.quantity)">
                                            <div class="predictive-search_product-item_styles" style="--flavor-bg: {{ product.metafields.custom.flavor_metaobject.value.background_color }};">
                                              <div class="predictive-search_line-item">
                                                <div class="predictive-search_image-wrapper">
                                                  <div class="product-item_radial is-mini"></div><img li-object:src="product.featured_media | image_url: width: 150" li-object:alt="product.featured_image.alt" alt="{{ product.featured_image.alt }}" src="{{ product.featured_media | image_url: width: 150 }}" loading="lazy" class="predictive-search_image">
                                                </div>
                                                <div class="product-item_bg is-mini"></div>
                                                <div class="predictive-search_product-info">
                                                  <div class="predictive-search_product_top">
                                                    <p data-wf--eyebrow--variant="small" li-object="product.metafields.custom.flavor_metaobject.value.flavor" class="g_eyebrow_txt u-text-style-h6 w-variant-2ea84ea1-fdee-b528-5a8e-fd7b483b99f5" style="background-color: {{ product.metafields.custom.flavor_metaobject.value.background_color }}; color: {{ product.metafields.custom.flavor_metaobject.value.text_color }};">{{ product.metafields.custom.flavor_metaobject.value.flavor }}</p>
                                                    <p data-wf--eyebrow--variant="small" class="g_eyebrow_txt u-text-style-h6 w-variant-2ea84ea1-fdee-b528-5a8e-fd7b483b99f5">24 Packs</p>
                                                  </div>
                                                  <div class="predictive-search_product_center">
                                                    <div li-object="product.title" class="predictive-search_product_title u-text-style-h6">{{ product.title }}</div>
                                                    <div li-object="product.metafields.custom.tagline" class="predictive-search_product_byline u-text-style-h6 u-weight-medium u-color-faded">{{ product.metafields.custom.tagline }}</div>
                                                  </div>
                                                  <div class="clickable_wrap u-cover-absolute">
                                                    <a li-object:href="product.url" class="clickable_link" href="{{ product.url }}"><span li-object="product.title" class="clickable_text u-sr-only">{{ product.title }}</span></a><button type="button" class="clickable_btn"><span li-object="product.title" class="clickable_text u-sr-only">{{ product.title }}</span></button>
                                                  </div>
                                                  <div class="predictive-search_product_bottom">
                                                    <div data-wf--button-main--variant="primary" class="button_main_wrap" data-button=" " data-trigger="hover focus">
                                                      <div class="clickable_wrap u-cover-absolute">
                                                        <a li-element="add-to-cart" class="clickable_link" @click="addToCart($event, true)" :disabled="!addToCartButton"><span class="clickable_text u-sr-only">Add to Cart</span></a><button type="button" class="clickable_btn"><span class="clickable_text u-sr-only">Add to Cart</span></button>
                                                      </div>
                                                      <div class="button_main_element">
                                                        <p aria-hidden="true" class="button_main_text u-text-style-h6">Add to Cart</p>
                                                        <div class="button_main_icon u-hide-if-empty"></div>
                                                        <div class="button_main_line"></div>
                                                      </div>
                                                      <div class="u-display-contents"></div>
                                                    </div>
                                                    <div class="predictive-search_price_wrap">
                                                      <div li-object="product.price | money" class="predictive-search_price u-text-style-small">{{ product.price | money }}</div>
                                                      {% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price">
    
                                                        <div li-object="product.compare_at_price | money" class="predictive-search_compare-price u-text-style-small u-color-faded">{{ product.compare_at_price | money }}</div>
                                                          </div>
{% endif %}
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                            </div>
                                          </div>
    
                                        
</div>
{% endfor %}
                                      </div>
                                    </div>
                                  </div>
                                    </div>
{% endunless %} {% schema %} {
    "tag": "div",
    "name": "predictive_search",
    "class": "predictive-search_section",
    "blocks": [
        {
            "name": "Search - Suggestion",
            "type": "search-suggestion",
            "settings": [
                {
                    "id": "url_link",
                    "type": "url",
                    "label": "Link"
                },
                {
                    "id": "text_text",
                    "type": "text",
                    "label": "Text",
                    "default": "Tag"
                }
            ]
        }
    ],
    "settings": [
        {
            "id": "collection_search_bestseller",
            "type": "collection",
            "label": "Search - Bestseller"
        },
        {
            "type": "range",
            "id": "collection_search_bestseller_limit",
            "min": 1,
            "max": 99,
            "step": 1,
            "label": "Limit: Search - Bestseller",
            "default": 6
        }
    ],
    "presets": [
        {
            "name": "predictive_search",
            "category": "Liquify",
            "blocks": [
                {
                    "type": "search-suggestion"
                }
            ]
        }
    ]
} {% endschema %}