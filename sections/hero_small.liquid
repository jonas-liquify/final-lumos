{{ section.settings.text_header_text }}
</h1>
                  </div>
                  <div class="hero_button_wrap u-margin-top-4">
                    {% for block in section.blocks %}
    {% if block.type == "button" %}
                    <div data-wf--button-main--variant="primary" li-block="Button" class="button_main_wrap" data-button=" " data-trigger="hover focus">
                                    
                      
                                    <div class="clickable_wrap u-cover-absolute">
                        <a class="clickable_link" href="{{ block.settings.url_link }}" target="{% if section.settings.open_in_new_tab %}_blank{% endif %}"> <span class="clickable_text u-sr-only">{{ block.settings.text_button_text }}</span> </a><button type="{{section.settings.button_type}}" class="clickable_btn"><span class="clickable_text u-sr-only">
{{ block.settings.text_button_text }}
</span></button>
                      </div>
                                    
                      
                                    <div class="button_main_element {{section.settings.variant}}">
                        <p aria-hidden="true" class="button_main_text u-text-style-h6">
{{ block.settings.text_button_text }}
</p>
                        <div class="button_main_icon u-hide-if-empty"></div>
                        <div class="button_main_line {{section.settings.variant}}"></div>
                      </div>
                                    
                      
                                    <div class="u-display-contents">
                        
                      </div>
                                    
                    
                            </div>
            {% endif %}
{% endfor %}
                  </div>
                </div>
              </div>
