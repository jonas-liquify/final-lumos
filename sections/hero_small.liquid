                  <div>
                    <div class="hero_eyebrow_wrap u-margin-bottom-3">
                      <p data-wf--eyebrow--variant="base" class="g_eyebrow_txt u-text-style-h6 u-display-inline-block">
{{ section.settings.text_eyebrow_text }}
</p>
                    </div>
                  </div>
                  <div style="max-width: {{section.settings.header_max_width | append: 'em'}}" class="hero_header_wrap">
